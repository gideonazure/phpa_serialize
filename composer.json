{
  "name": "phpa/serialize_class",
  "description": "This is a project for class serialization",
  "keywords": ["project", "classes", "serialize"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexey Sk",
      "email": "gid.azure@gmail.com",
      "role": "Maintainer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-check": "php-cs-fixer fix --allow-risky=yes --diff --ansi --dry-run",
    "cs-fix": "php-cs-fixer fix --allow-risky=yes --diff --ansi",
    "tests": "phpunit --colors=always"
  },
  "config": {
    "sort-packages": true
  }
}
